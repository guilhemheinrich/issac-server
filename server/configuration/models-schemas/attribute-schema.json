{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://issac/attribute",
    "title": "Attribute",
    "description": "Entity representing an attribute",
    "type": "object",
    "properties": {
        "key": {
            "description": "The key/identifier of the attribute",
            "type": "string"
        },
        "type": {
            "description": "The awaited type of the attributes"
        }
    },
    "required": [
        "key", "type"
    ]
}