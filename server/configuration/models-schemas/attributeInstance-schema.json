{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://issac/attributeInstance",
    "title": "AttributeInstance",
    "description": "Entity representing an instance of an attribute",
    "type": "object",
    "properties": {
        "key": {
            "description": "The key/identifier of the attribute",
            "type": "string"
        },
        "value": {
            "description": "The value of the field"
        }
    },
    "required": [
        "key", "value"
    ]
}