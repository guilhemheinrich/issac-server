{
    "Processus": {
        "namedGraph": "context_processus_added",
        "permissions": ["read", "write"],
        "namespaces": ["issac", "rdfs", "skos"],
        "Pkey": "uri",
        "objects": {
            "uri": {
                "object": "issac:Processus"
            },
            "name": {
                "predicate": "rdfs:label"
            },
            "description": {
                "predicate": "skos:definition"
            },
            "agents": {
                "predicate": "issac:involve"
            },
            "owners": {
                "predicate": "issac:hasOwner"
            },
            "context": {
                "predicate": "issac:hasContext"
            }
        }
    },
    "Context": {
        "namedGraph": "context_processus_added",
        "namespaces": "issac",
        "permissions": ["read", "write"],
        "objects": {
            "uri": {
                "object": "issac:Context"
            },
            "location": {
                "predicate": "issac:hasLocation"
            },
            "primaryPlant": {
                "predicate": "issac:hasPrimaryPlant"
            }
        }
    },
    "Agent": {
        "namedGraph": "context_administration",
        "permissions": ["read", "write"],
        "namespaces": "foaf",
        "objects": {
            "uri": {
                "object": "foaf:Person"
            },
            "first_name": {
                "predicate": "foaf:firstName"
            },
            "last_name": {
                "predicate": "foaf:lastName"
            },
            "email": {
                "predicate": "foaf:mbox"
            },
            "group": {
                "predicate": "foaf:member"
            }
        }
    },
    "SKOSnode": {
        "permissions": ["read"],
        "namespaces": "skos",
        "objects": {
            "uri": {
                "object": "skos:Concept"
            },
            "label": {
                "predicate": "skos:prefLabel"
            }
        }
    }
}